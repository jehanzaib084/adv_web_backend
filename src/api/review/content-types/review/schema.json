{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "review"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "comment": {
      "type": "blocks"
    },
    "reviewed_at": {
      "type": "datetime"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
